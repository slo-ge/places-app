{
  "name": "export-latest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:local-api": "ng serve --proxy-config proxy.conf-local.json",
    "start:local": "ng serve --open --host 0.0.0.0 --disable-host-check --proxy-config proxy.conf.json",
    "build": "ng build --configuration production",
    "build:dev": "ng build",
    "build-base": "ng build --configuration qa --base-href https://static.phipluspi.com/meta-mapper/ && node ftp/deploy.js",
    "lint": "ng lint",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/export-latest/stats.json",
    "wrangler": "wrangler pages dev dist/export-latest --d1=DB --local --persist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.0.2",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "~17.0.2",
    "@angular/compiler": "~17.0.2",
    "@angular/core": "~17.0.2",
    "@angular/forms": "~17.0.2",
    "@angular/platform-browser": "~17.0.2",
    "@angular/platform-browser-dynamic": "~17.0.2",
    "@angular/router": "~17.0.2",
    "@fortawesome/angular-fontawesome": "^0.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@sentry/angular-ivy": "^7.51.2",
    "@sentry/tracing": "^7.51.2",
    "@types/fabric": "^5.3.2",
    "@types/fontfaceobserver": "0.0.6",
    "fabric": "^5.3.0",
    "fontfaceobserver": "^2.3.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.4.1",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.0.0",
    "@angular-eslint/builder": "17.0.1",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/schematics": "17.0.1",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "~17.0.2",
    "@cloudflare/workers-types": "^4.20221111.1",
    "@types/dom-mediacapture-record": "1.0.7",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.23.1",
    "prettier": "2.8.8",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.3",
    "typescript": "^5.2.2",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
