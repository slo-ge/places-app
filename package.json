{
    "name": "goove",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:local": "ng serve --open --host 0.0.0.0 --disable-host-check",
        "build": "npm run increase:version && ng build --prod",
        "build:withstats": "ng build --prod --stats-json",
        "build:full": "npm run build && npm run copy:htaccess",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "increase:version": "node scripts/write-version.js",
        "copy:htaccess": "copy ..\\.htaccess dist\\goove",
        "bundle-report": "webpack-bundle-analyzer dist/goove/stats-es2015.json",
        "postinstall": "ngcc --properties es2015 browser module main --first-only",
        "dev:ssr": "ng run goove:serve-ssr",
        "serve:ssr": "node dist/goove/server/main.js",
        "build:ssr": "ng build --prod && ng run goove:server:production",
        "prerender": "ng run goove:prerender"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^11.0.9",
        "@angular/cdk": "^9.2.4",
        "@angular/common": "^11.0.9",
        "@angular/compiler": "^11.2.14",
        "@angular/core": "^11.2.14",
        "@angular/forms": "^11.0.9",
        "@angular/platform-browser": "^11.0.9",
        "@angular/platform-browser-dynamic": "^11.0.9",
        "@angular/platform-server": "^11.2.14",
        "@angular/router": "^11.0.9",
        "@angular/service-worker": "^11.0.9",
        "@nguniversal/express-engine": "^11.2.1",
        "@ngx-share/core": "^7.1.4",
        "@ngxs/storage-plugin": "^3.7.2",
        "@ngxs/store": "^3.7.2",
        "apicache": "^1.6.2",
        "express": "^4.15.2",
        "rxjs": "^6.5.5",
        "tslib": "^2.2.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1100.7",
        "@angular/cli": "^11.0.7",
        "@angular/compiler-cli": "^11.2.10",
        "@angular/language-service": "^11.2.10",
        "@nguniversal/builders": "^9.0.0",
        "@types/express": "^4.17.12",
        "@types/jasmine": "^3.6.11",
        "@types/jasminewd2": "^2.0.9",
        "@types/node": "^12.20.14",
        "codelyzer": "^6.0.2",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.6.0",
        "protractor": "~7.0.0",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.0",
        "typescript": "^4.0.7",
        "webpack-bundle-analyzer": "^3.9.0"
    }
}
